{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "username": {
        "type": "string"
      },
      "password": {
        "type": "string"
      },
      "first_name": {
        "type": "string"
      },
      "last_name": {
        "type": "string"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "phone": {
        "type": "string",
        "minLength": 10,
        "maxLength": 15
      }
    },
    "required": [
      "username",
      "password",
      "first_name",
      "last_name",
      "email",
      "phone"
    ]
  }